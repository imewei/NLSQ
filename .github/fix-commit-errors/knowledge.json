      "id": "pre-commit-end-of-file-fixer-001",
      "pattern": "fix end of files\\.+Failed.*files were modified by this hook",
      "category": "pre-commit_hook_failure",
      "subcategory": "end_of_file_formatting",
      "root_cause": "Files missing EOF newlines when end-of-file-fixer hook requires exactly one newline at end of files",
      "confidence": 1.0,
      "solutions": [
        {
          "action": "add_eof_newlines",
          "description": "Add single newline at end of affected files",
          "command": "find <dir> -name \"<pattern>\" -type f -exec sh -c 'if [ -n \"$(tail -c 1 \"$1\")\" ]; then echo >> \"$1\"; fi' _ {} \\;",
          "success_rate": 1.0,
          "applications": 1,
          "risk_level": "none",
          "reversibility": "high"
        }
      ],
      "validation": {
        "command": "pre-commit run end-of-file-fixer --all-files",
        "expected_output": "Passed"
      },
      "last_seen": "2025-10-21T06:07:21Z",
      "occurrences": 1,
      "auto_fixable": true
    },
    {
      "id": "matplotlib-tkinter-backend-001",
      "pattern": "_tkinter\\.TclError: Can't find a usable init\\.tcl",
      "category": "test_failure",
      "subcategory": "matplotlib_backend_error",
      "root_cause": "Matplotlib attempting to use Tkinter backend in headless CI environment where Tkinter is not properly configured",
      "confidence": 1.0,
      "solutions": [
        {
          "action": "configure_matplotlib_agg_backend",
          "description": "Configure matplotlib to use Agg (non-interactive) backend before importing pyplot",
          "code_change": {
            "pattern": "import matplotlib.pyplot as plt",
            "replacement": "import matplotlib\\nmatplotlib.use('Agg')  # Use non-interactive backend for CI/headless environments\\nimport matplotlib.pyplot as plt"
